{
  "$schema": "https://raw.githubusercontent.com/grafana/grafana/main/docs/sources/developers/plugins/plugin.schema.json",
  "name": "InfinityPlus",
  "id": "infinity-plus-datasource",
  "type": "datasource",
  "executable": "gpx_infinityplus",
  "backend": true,
  "metrics": true,
  "annotations": true,
  "alerting": true,
  "logs": true,
  "tracing": true,
  "info": {
    "version": "%VERSION%",
    "updated": "%TODAY%",
    "description": "Enhanced JSON API, CSV, TSV, XML, GraphQL, HTML, Google Sheets and HTTP/REST API datasource for Grafana with additional features and integrations. Transform data with UQL/GROQ. Visualize data from many APIs including Amazon AWS, Microsoft Azure, Google Cloud / GCP, and RSS/ATOM feeds directly. Also supports visualizing logs and traces.",
    "author": {
      "name": "Cloud Rhino",
      "url": "https://cloudrhino.netlify.app"
    },
    "logos": {
      "small": "img/icon.png",
      "large": "img/icon.png"
    },
    "keywords": [
      "api",
      "datasource",
      "infinity",
      "grafana",
      "json",
      "csv",
      "xml",
      "graphql",
      "html",
      "rest api",
      "cloud",
      "aws",
      "azure",
      "gcp",
      "uql",
      "groq"
    ],
    "screenshots": [
      {
        "name": "features",
        "path": "img/slide-features.png"
      },
      {
        "name": "rest-api",
        "path": "img/slide-rest-api.png"
      },
      {
        "name": "variables",
        "path": "img/slide-variables.png"
      },
      {
        "name": "series",
        "path": "img/slide-series.png"
      }
    ],
    "links": [
      {
        "name": "Docs",
        "url": "https://cloudrhino.netlify.app/docs/infinityplus"
      },
      {
        "name": "Github",
        "url": "https://github.com/cloudrhino/infinity-plus-datasource"
      },
      {
        "name": "Need Help?",
        "url": "https://github.com/cloudrhino/infinity-plus-datasource/discussions"
      },
      {
        "name": "Report Bug",
        "url": "https://github.com/cloudrhino/infinity-plus-datasource/issues/new/choose"
      },
      {
        "name": "Examples",
        "url": "https://github.com/cloudrhino/infinity-plus-datasource/discussions/categories/examples"
      }
    ],
    "references": [
      {
        "type": "original",
        "name": "Based on the original Infinity datasource by Grafana Labs",
        "url": "https://grafana.com/grafana/plugins/yesoreyeram-infinity-datasource"
      }
    ]
  },
  "dependencies": {
    "grafanaDependency": ">=9.5.15",
    "grafanaVersion": "9.5.x",
    "plugins": []
  },
  "includes": [],
  "routes": []
}
